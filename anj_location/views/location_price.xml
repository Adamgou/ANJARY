        <record id="view_location_price_view" model="ir.ui.view">
            <field name="name">mds_form.form</field>
            <field name="model">location.price</field>
            <field name="arch" type="xml">
                <form string="Price rental" >
                    <sheet>
                        <field name="name"/>
                        <field name="location_price"/>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="view_location_price_tree" model="ir.ui.view">
            <field name="name">location_price.tree</field>
            <field name="model">location.price</field>
            <field name="arch" type="xml">
                <tree string="Abonnement">
                    <field name="name"/>
                        <field name="location_price"/>
                </tree>
            </field>
        </record>
    
        <record model="ir.actions.act_window" id="action_location_price">
            <field name="name">price location</field>
            <field name="res_model">location.price</field>
            <field name="view_mode">tree,form</field>
            <!-- <field name="context">{"search_default_sector_id":1,"search_default_directory_id":1}</field> -->
        </record>
        <menuitem id="location_price" name="Location price configurations"  action="action_location_price" parent="stock.menu_stock_config_settings"/>
    </data>
</odoo>