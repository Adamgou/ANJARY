<?xml version="1.0" encoding="UTF-8"?>

<odoo>
  <record id="pos_config_view_form" model="ir.ui.view">
    <field name="name">pos.config.view.form.inherit.anj_security</field>
    <field name="model">pos.config</field>
    <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
    <field name="arch" type="xml">
      <setting id="other_devices" position="after">
        <setting id="security" string="Secutiry" help="Unauthorized Security">
          <field name="unauthorized_user_ids" widget="many2many_avatar_user" />
        </setting>
      </setting>
    </field>
  </record>
</odoo>
