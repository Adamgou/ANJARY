<odoo>
    <data>
        <record id="anj_account_res_partner_view_form" model="ir.ui.view">
            <field name="name">anj.account.res.partner.form.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form" />
            <field name="arch" type="xml">
                <xpath expr="//group[@name='banks']" position="after">
                    <group string="sequence">
                        <field name="sequence_started" invisible="1" />
                        <field name="invoice_prefix" attrs="{'readonly': [('sequence_started', '=', True)]}"/>
                    </group>
                </xpath>
                <xpath expr="//page[@name='accounting_disabled']/div" position="before">
                    <group string="sequence">
                        <field name="sequence_started" invisible="1" />
                        <field name="invoice_prefix" attrs="{'readonly': [('sequence_started', '=', True)]}"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>