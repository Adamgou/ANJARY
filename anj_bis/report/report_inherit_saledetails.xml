						<t t-if="currency['position']">
							<span t-esc="royality_ht_biskot" t-options="{'widget': 'float', 'precision': currency['precision']}"/>
							<span t-out='currency["symbol"]'>$</span>
						</t>
						<t t-else="">
							<span t-out='currency["symbol"]'>$</span>
							<span t-esc="royality_ht_biskot" t-options="{'widget': 'float', 'precision': currency['precision']}"/>
						</t>
					</td>
					<td class="text-end"/>
				</tr>
			</xpath>
			<xpath expr="//t[@id='taxe_sales']/table/tbody/tr[last()]" position="inside">
				<td class="text-end" style="background-color: #b1b1b1;">
					<t t-if="currency['position']">
						<strong>
							<span t-out='products_info["all_amount_incl_vat"]'
							      t-options="{'widget': 'float', 'precision': currency['precision']}">0.00
							</span>
							<span t-out='currency["symbol"]'>$</span>
						</strong>
					</t>
					<t t-else="">
						<strong>
							<span t-out='currency["symbol"]'>$</span>
							<span t-out='products_info["all_amount_incl_vat"]'
							      t-options="{'widget': 'float', 'precision': currency['precision']}">0.00
							</span>
						</strong>
					</t>
				</td>
			</xpath>
			<xpath expr="//div[@class='oe_structure']" position="replace"/>
			<xpath expr="//div[@class='page']/br[4]" position="replace"/>
			<xpath expr="//t[@id='payments']" position="before">
				<div t-if="company.is_biskot" style="margin-top: -20px">
					<t t-set="average_total_basket" t-value="(currency['total_paid']/nbr_orders) if nbr_orders else 0"/>
					<t t-set="average_total_basket_biskot" t-value="(ttc_biskot/nbr_orders) if nbr_orders else 0"/>
					<t t-set="average_total_basket_spoon" t-value="(ttc_spoon/nbr_orders) if nbr_orders else 0"/>
					<div>
						<span>Total average basket:</span>
						<span t-esc="average_total_basket" t-options="{'widget': 'float', 'precision': currency['precision']}"/>
						<span t-out='currency["symbol"]'>$</span>
					</div>
					<div>
						<span>Total average basket Biskot:</span>
						<span t-esc="average_total_basket_biskot"
						      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
						<span t-out='currency["symbol"]'>$</span>
					</div>
					<div>
						<span>Total average basket speon:</span>
						<span t-esc="average_total_basket_spoon"
						      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
						<span t-out='currency["symbol"]'>$</span>
					</div>
				</div>
			</xpath>
			<xpath expr="//div[@id='discounts']" position="replace"/>
			<xpath expr="//t[@id='closing_session']/h3" position="attributes">
				<attribute name="style">margin-top: -30px;</attribute>
			</xpath>
			<xpath expr="//t[@id='closing_session']/br[1]" position="replace"/>
			<xpath expr="//t[@id='closing_session']/br[2]" position="replace"/>
			<xpath expr="//t[@id='closing_session']/div[2]/strong[2]" position="replace">
				<t t-if="company.is_biskot">
					<strong>Receipt number:
						<span t-out="nbr_orders">10</span>
					</strong>
				</t>
				<t t-else="">
					<strong>Number of transactions:
						<span t-out="nbr_orders">10</span>
					</strong>
				</t>
			</xpath>
			<xpath expr="//t[@id='closing_session']" position="after">
				<t t-if="company.is_biskot">
					<table class="table table-bordered" style="page-break-inside: avoid;">
						<tr style="background-color: #b1b1b1;">
							<td/>
							<td class="text-center">Espèces</td>
							<td class="text-center">CB</td>
							<td class="text-center">MVOLA</td>
						</tr>
						<tr>
							<td style="background-color: #b1b1b1;">CA TTC BISKOT</td>
							<td class="text-center">
								<span t-esc="ttc_biskot - (payments_by_method['mvola'] + payments_by_method['CB'])"
								      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
								<span t-out='currency["symbol"]'>$</span>
							</td>
							<td class="text-center">
								<span t-esc="payments_by_method['CB']"
								      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
								<span t-out='currency["symbol"]'>$</span>
							</td>
							<td class="text-center">
								<span t-esc="payments_by_method['mvola']"
								      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
								<span t-out='currency["symbol"]'>$</span>
							</td>
						</tr>
						<tr>
							<td style="background-color: #b1b1b1;">CA TTC SPOON</td>
							<td class="text-center">
								<span t-esc="ttc_spoon" t-options="{'widget': 'float', 'precision': currency['precision']}"/>
								<span t-out='currency["symbol"]'>$</span>
							</td>
							<td/>
							<td/>
						</tr>
						<tr>
							<td style="background-color: #b1b1b1;">Total CA TTC</td>
							<td class="text-center">
								<span t-esc="ttc_biskot + ttc_spoon"
								      t-options="{'widget': 'float', 'precision': currency['precision']}"/>
								<span t-out='currency["symbol"]'>$</span>
							</td>
							<td/>
							<td/>
						</tr>
					</table>
					<div>
						<t t-if="pricelists">
							<strong>Sales revenues:</strong>
							<t t-foreach="pricelists.items()" t-as="lists">
								<t t-foreach="lists[1].items()" t-as="item">
									<div>
										<span t-esc="item[0]"/>:
										<span t-esc="item[1]" t-options="{'widget': 'float', 'precision': currency['precision']}"/>
										<span t-out='currency["symbol"]'>$</span>
									</div>
								</t>
							</t>
						</t>
					</div>
				</t>
			</xpath>
			<xpath expr="//t[@id='closing_session']/table" position="attributes">
				<attribute name="style">margin-top: -25px</attribute>
			</xpath>
		</template>
	</data>
</odoo>
